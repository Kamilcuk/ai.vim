Execute (prepare):
  function Myget()
    let tmp = getline(1, '$')
    let tmp = join(tmp, " ")
    let tmp = substitute(tmp, "  *", " ", "g")
    let tmp = trim(tmp)
    return tmp
  endfunction
  let g:ai_vader = v:true

Execute (normal):
  let g:ai_mock = "1 2"
  AI write 1 2
  AssertEqual Myget(), '1 2'


Execute (chat):
  let g:ai_mock = "1 2"
  AI! write 1 2
  AssertEqual Myget(), '1 2'


Given (percent):
  1
  2
  3
Execute (percent):
  let g:ai_mock = "2"
  %AI replace by 2
  AssertEqual Myget(), '2'


Given (range):
  1
  2
Execute (range):
  let g:ai_mock = "2"
  1,1AI replace 1 by 2
  AssertEqual Myget(), '2 2'


